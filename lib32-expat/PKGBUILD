pkgname=lib32-expat
pkgver=2.0.1
pkgrel=5
pkgdesc="An XML Parser library written in C"
url="http://expat.sourceforge.net/"
license=('custom')
arch=(x86_64)
groups=('lib32')
depends=('lib32-glibc')
source=("http://chakra-project.org/repo/core-testing/i686/expat-${pkgver}-${pkgrel}-i686.pkg.tar.xz")
md5sums=('29e56e1ac3c8534ef0d7d62239a9a322')

build() {
	cd ${srcdir}
	mkdir -p $startdir/pkg/opt/lib32/usr/lib
	cp -dp usr/lib/*.so* $startdir/pkg/opt/lib32/usr/lib
}

