_pkgsourcename=expat
pkgname=lib32-$_pkgsourcename
pkgver=2.0.1
pkgrel=5
pkgdesc="An XML Parser library written in C"
url="http://expat.sourceforge.net/"
license=('custom')
arch=(x86_64)
groups=('lib32')
depends=('lib32-glibc')
source=(http://www.chakra-project.org/repo/core/i686/${pkgname/lib32-/}-${pkgver}-${pkgrel}-i686.pkg.tar.xz)

build() {
	cd $startdir/src
	mkdir -p $startdir/pkg/opt/lib32/usr/lib
	cp -dp usr/lib/*.so* $startdir/pkg/opt/lib32/usr/lib
}
md5sums=('29e56e1ac3c8534ef0d7d62239a9a322')
